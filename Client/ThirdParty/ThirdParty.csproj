<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>$(ClientTargetFrameworks)</TargetFrameworks>
        <LangVersion>$(ClientLanguageVersion)</LangVersion>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>annotations</Nullable>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="CommandLineParser" Version="2.9.1"/>
        <PackageReference Include="MemoryPack" Version="1.9.16" />
        <PackageReference Include="MongoDB.Bson" Version="2.17.1"/>
        <PackageReference Include="MongoDB.Driver.Core" Version="2.17.1"/>
        <PackageReference Include="SharpZipLib" Version="1.4.2" />
        <PackageReference Include="NLog" Version="5.2.4"/>
    </ItemGroup>

</Project>
